# env/elastic.env.example
# Copy to env/elastic.env (gitignored) and set strong secrets.

ELASTIC_CLUSTER_NAME=drp-elk
ELASTIC_VERSION=9.2.4

# Built-in superuser password
ELASTIC_PASSWORD=ChangeMe_StrongPassword

# Optional: for keystore / cert password if you choose to protect PKCS#12 files
P12_PASSWORD=ChangeMe_P12Password

# Paths (host paths used by docker-compose files)
ES_BASE_DIR=/opt/elastic

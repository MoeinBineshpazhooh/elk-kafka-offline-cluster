# env/kibana.env.example
# Copy to env/kibana.env (gitignored)

KIBANA_VERSION=9.2.4

KIBANA_SERVER_HOST=0.0.0.0
KIBANA_SERVER_PORT=5601

# Use ES HTTPS endpoint(s)
ELASTICSEARCH_HOSTS=["https://es01.drp.local:9200","https://es02.drp.local:9200","https://es03.drp.local:9200"]

ELASTICSEARCH_USERNAME=kibana_system
ELASTICSEARCH_PASSWORD=ChangeMe_KibanaSystemPassword

# CA cert path inside Kibana container (volume-mounted in compose)
ELASTICSEARCH_SSL_CERTIFICATEAUTHORITIES=/usr/share/kibana/config/certs/ca/ca.crt
ELASTICSEARCH_SSL_VERIFICATIONMODE=certificate

# Required encryption keys (must be 32+ chars; keep them stable)
XPACK_SECURITY_ENCRYPTIONKEY=abcdefghijklmnopqrstuvwxyz123456
XPACK_ENCRYPTEDSAVEDOBJECTS_ENCRYPTIONKEY=abcdefghijklmnopqrstuvwxyz123456
XPACK_REPORTING_ENCRYPTIONKEY=abcdefghijklmnopqrstuvwxyz123456

# env/kafka-controller.env.example
# Copy to env/kafka-controller.env (gitignored)

KAFKA_IMAGE=apache/kafka:latest

# Same cluster id on all controllers/brokers (generated once, reused forever)
KAFKA_CLUSTER_ID=REPLACE_WITH_REAL_CLUSTER_ID

# Controller quorum voters (must match node ids and IPs)
CONTROLLER_QUORUM_VOTERS=1@10.10.4.121:9093,2@10.10.4.122:9093,3@10.10.4.123:9093
